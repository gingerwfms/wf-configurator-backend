{
    "name": "gingerwfms/wf-configurator-backend",
    "type": "ginger-backend-plugin",
    "description": "The official Workflow Configurator Backend for the Ginger WfMS",
    "license": "BSD-3-Clause",    
    "homepage": "http://www.codeliner.ws/",
    "authors": [
        {
            "name": "Alexander Miertsch",
            "email": "kontakt@codeliner.ws",
            "homepage": "http://www.codeliner.ws/"
        }
    ],
    "keywords": [
        "ginger",
        "wfms",
        "backend",
        "workflow",
        "management"
    ],
    "require": {
        "php": ">=5.4.0",
        "gingerwfms/ginger-plugin-installer" : "dev-master"
    },
    "require-dev" : {
        "phpunit/phpunit": "3.7.*",
        "zendframework/zendframework": "2.2.*",
        "malocher/cqrs-esb" : "1.*",
        "malocher/zf2-cqrs-module" : "1.*",
        "gingerwfms/ginger-core" : "dev-master"
    },
    "autoload": {
        "psr-0": {            
            "GingerWfConfigBackend\\": "src",
            "GingerWfConfigBackendTest\\": "tests"
        }
    },
    "extra": {
        "plugin-namespace": "GingerWfConfigBackend"
    },
    "minimum-stability" : "dev"
}
